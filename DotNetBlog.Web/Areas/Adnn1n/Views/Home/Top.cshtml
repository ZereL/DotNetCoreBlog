@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Top</title>
    <link href="/adminstyle/css/style.css" rel="stylesheet" type="text/css" />
    <script language="JavaScript" src="/adminstyle/js/jquery.js"></script>
    <script type="text/javascript">
$(function(){
	$(".nav li a").click(function(){
		$(".nav li a.selected").removeClass("selected")
		$(this).addClass("selected");
	})
})
    </script>

</head>
<body style="background:url(/adminstyle/images/topbg.gif) repeat-x;">
    <div class="topleft">
        <a href="main.html" target="_parent"><img src="/adminstyle/images/logo.png" title="系统首页" /></a>
    </div>
    <ul class="nav">
        <li><a href="/Adnn1n/Home/Welcome" target="rightFrame" class="selected"><img src="/adminstyle/images/icon01.png" title="Welcome" /><h2>Welcome</h2></a></li>
        <li><a href="/Adnn1n/Admin/ModPwd" target="rightFrame"><img src="/adminstyle/images/icon06.png" title="ChangePassWord" /><h2>Change PassWord</h2></a></li>
    </ul>
    <div class="topright">
        <ul>
            <li><a href="/Adnn1n/Login" target="_parent">Logout</a></li>
        </ul>
        <div class="user">
            <span>@ViewBag.username</span>
        </div>
    </div>
</body>
</html>
